type User {
  id: ID!
  name: String!
  email: String!
  groups: [Group!]!
  teams: [Team!]!
}

type Group {
  id: ID!
  name: String!
  teams: [Team!]!
  users: [User!]!
}

type Team {
  id: ID!
  name: String!
  group: Group!
  users: [User!]!
}

type Location {
  id: ID!
  name: String!
}

type AuthResponse {
  token: String!
  user: User!
}

type Sport {
  id: ID!
  name: String!
  weight: Int!
}

type Scene {
  id: ID!
  name: String!
}
